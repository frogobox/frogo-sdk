<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frogo_rv_list_type_3_root_linear_layout"
    style="@style/FrogoLayoutList8dp"
    android:orientation="vertical">

    <TextView
        android:id="@+id/frogo_rv_list_type_3_tv_title"
        style="@style/frogoTvListTitle"
        tools:text="@tools:sample/lorem/random" />

    <TextView
        android:id="@+id/frogo_rv_list_type_3_tv_subtitle"
        style="@style/FrogoTvListSubTitle"
        tools:text="@tools:sample/lorem/random" />

    <TextView
        android:id="@+id/frogo_rv_list_type_3_tv_desc"
        style="@style/FrogoTvListDescription"
        android:layout_marginTop="@dimen/frogo_dimen_8dp"
        tools:text="@tools:sample/lorem/random" />

</LinearLayout>